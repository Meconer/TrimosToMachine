        <tabs>
          <Tab fx:id="fanucMachineTab" onSelectionChanged="#machineTabChanged" text="Fanuc">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="387.0" prefWidth="408.0">
                     <children>
                        <VBox layoutX="14.0" layoutY="14.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
                           <children>
                              <Label prefHeight="17.0" prefWidth="93.0" text="Verktygsnr" />
                              <HBox spacing="10.0">
                                 <children>
                                    <TextField fx:id="fanucToolNoTextField" promptText="1" />
                                    <Button fx:id="fanucSetToOneButton" mnemonicParsing="false" onAction="#fanucSetToOneButtonClicked" prefHeight="25.0" prefWidth="117.0" text="Sätt till 1" />
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </VBox.margin>
                              </HBox>
                              <HBox prefHeight="25.0" prefWidth="156.0" spacing="90.0">
                                 <children>
                                    <Button fx:id="fanucIncToolNoButton" mnemonicParsing="false" onAction="#fanucIncToolNo" prefWidth="30.0" text="+">
                                       <HBox.margin>
                                          <Insets left="5.0" />
                                       </HBox.margin>
                                    </Button>
                                    <Button fx:id="fanucDecToolNoButton" mnemonicParsing="false" onAction="#fanucDecToolNo" prefHeight="25.0" prefWidth="30.0" text="-" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                        <CheckBox fx:id="fanucUseRadiusCheckBox" layoutX="21.0" layoutY="112.0" mnemonicParsing="false" onAction="#useRadiusCheckBoxChanged" text="Använd radien" />
                        <CheckBox fx:id="fanucSetRadiusToZeroCheckBox" disable="true" layoutX="18.0" layoutY="148.0" mnemonicParsing="false" text="Sätt radien till 0" />
                        <Button layoutX="194.0" layoutY="108.0" mnemonicParsing="false" onAction="#fanucAddStartSection" prefHeight="25.0" prefWidth="188.0" text="Start" />
                        <Button layoutX="194.0" layoutY="156.0" mnemonicParsing="false" onAction="#fanucAddEndSection" prefHeight="25.0" prefWidth="188.0" text="Avslutning" />
                        <VBox layoutX="14.0" layoutY="199.0">
                           <children>
                              <Label text="Extra längdoffset" />
                              <TextField fx:id="fanucExtraLengthOffset" text="1.00" />
                           </children>
                        </VBox>
                        <VBox layoutX="14.0" layoutY="253.0">
                           <children>
                              <Label text="Extra radieoffset" />
                              <TextField fx:id="fanucExtraRadiusOffset" text="0.0" />
                           </children>
                        </VBox>
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Untitled Tab 2">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <AnchorPane layoutX="14.0" layoutY="49.0" prefHeight="343.0" prefWidth="177.0">
         <children>
            <Label layoutX="16.0" layoutY="14.0" prefHeight="47.0" prefWidth="289.0" text="0.00" AnchorPane.leftAnchor="16.0" AnchorPane.rightAnchor="19.0" fx:id="xLabel">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Label fx:id="zLabel" layoutX="16.0" layoutY="79.0" prefHeight="47.0" prefWidth="176.0" text="0.00" AnchorPane.leftAnchor="16.0" AnchorPane.rightAnchor="19.0" AnchorPane.topAnchor="79.0">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button layoutX="611.0" layoutY="335.0" mnemonicParsing="false" onAction="#clearTextArea" prefHeight="25.0" prefWidth="142.0" text="Rensa" />
      <Button layoutX="611.0" layoutY="367.0" mnemonicParsing="false" onAction="#removeLastReadLine" text="Ångra senaste inläsning" />
   </children>
</AnchorPane>

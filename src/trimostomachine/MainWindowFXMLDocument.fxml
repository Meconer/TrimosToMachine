<AnchorPane id="AnchorPane" prefHeight="492.0" prefWidth="939.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="trimostomachine.MainWindowFXMLController">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="980.0">
        <menus>
          <Menu mnemonicParsing="false" text="Arkiv">
            <items>
              <MenuItem mnemonicParsing="false" text="Stäng" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Verktyg">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onMenuSettingsClicked" text="Inställningar" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Hjälp">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#showAboutBox" text="Om" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TextArea fx:id="toolFileTextArea" layoutX="611.0" layoutY="49.0" prefHeight="267.0" prefWidth="314.0" />
